<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.locuslabs.com.uiexamplelibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Example Library" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExampleChangeFont"
            android:label="Example - Change search bar font">
        </activity>
        <activity
            android:name=".ExampleChangeBackgroundColor"
            android:label="Example - Change background color on search view">
        </activity>
        <activity
            android:name=".ExampleChangeLevelsButtonTheme"
            android:label="Example - Change botton bar theme ">
        </activity>
    </application>

</manifest>